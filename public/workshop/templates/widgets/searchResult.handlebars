<h3>Search Result Widget!</h3>
<ol class="searchResultContainer">
  {{#each searchResults}}
    <li class="searchResult" data-result-latitude={{roomOffer.flatshare.geoLocation.latitude}} data-result-longitude={{roomOffer.flatshare.geoLocation.longitude}}>
      <div class="searchResultFront">
        <div>Current Distance: {{currentDistance}}&nbsp;km</div>
        <div>MatchingScore {{matchingScore.value}} - {{matchingScore.defined}}</div>

        {{#with roomOffer}}
          <div>{{roomDetails.totalRentPerMonthInEuro}}€</div>
    
          <div>
            <div>Zimmergröße:&nbsp;{{roomDetails.roomSize.squareMeters}}&nbsp;m²</div>
            <div>Zeitraum:
              {{#unless roomDetails.freeTo}}
                ab {{date roomDetails.freeFrom}}
              {{else}}
                {{date roomDetails.freeFrom}}&nbsp;bis&nbsp;{{date roomDetails.freeTo}}
              {{/unless}}
            </div>
          </div>
    
          <div>
            <div>gewünschtes Alter: {{criteria.minAge}}&nbsp;bis&nbsp;{{criteria.maxAge}}</div>
            <div>gewünschtes Geschlecht: {{criteria.genders}}</div>
          </div>
    
          <div>
            <div>Beschreibung: {{description}}</div>
            <div>Kontakt E-Mail: {{contactEmail}}</div>
            <div>Bewerbungen: {{applications}}</div>
            <form class="searchResultApplyForm"
                id="SearchResultApplyForm_{{id}}">
              <input type="hidden" name="id" value="{{id}}">
              <input type="submit" name="ApplyButton" value="Bewerben">
            </form>
          </div>
        {{/with}}
      </div>
      <div class="searchResultBack">
       Foo is silence
      </div>
    </li>
  {{/each}}
</ol>
